{
  "type": "object",
  "required": ["version", "collectors"],
  "properties": {
    "version": { "type": ["integer", "string"] },
    "updated": { "type": "string" },
    "collectors": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["id", "legal_name", "contacts"],
        "properties": {
          "id": { "type": "string", "minLength": 1 },
          "legal_name": { "type": "string", "minLength": 1 },
          "operating_names": { "type": "array", "items": { "type": "string" } },
          "licenses": {
            "type": "array",
            "items": {
              "type": "object",
              "required": ["province", "number"],
              "properties": {
                "province": { "type": "string" },
                "number": { "type": "string" },
                "expiry": { "type": "string" }
              },
              "additionalProperties": true
            }
          },
          "contacts": {
            "type": "object",
            "required": ["registered_address"],
            "properties": {
              "disputes_email": { "type": "string" },
              "complaints_email": { "type": "string" },
              "registered_address": { "type": "string" }
            },
            "additionalProperties": true
          }
        },
        "additionalProperties": true
      }
    }
  },
  "additionalProperties": true
}